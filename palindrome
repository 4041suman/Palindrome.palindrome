public class reverse {
    public static void main(String[] args) {
        int no= 1991;
       int  no2= no;
        int rem=0;
        while(no>0){
        rem = rem*10 + no%10;
        no = no/10;
        }
        System.out.println("reverse numbber of 1991 is : "+rem);
           if(rem==no2)
                System.out.println(rem+" is palindrome");
           else
                  System.out.println("not a palindrome");
       }
}
 
