public class reverse {
 public static void main(String[] args) {
   for(int i=100;i<999;i++){
      int no=i;
      int no2=no;
      int rem=0;
        while(no>0){
        rem = rem*10 + no%10;
        no = no/10;
        }
        
        if(rem==no2)
                System.out.println(rem+" is palindrome");
  }

}
}
